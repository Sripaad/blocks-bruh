class Block 
{
    constructor(index, previousHash, timestamp, data, hash)
    {
        this.index = index;
        this.previousHash = previousHash.toString();
        this.timestamp = timestamp;
        this.data = data;
        this.hash = hash.toString();
    }
}
//To keep things as simple as possible i have included only the most necessary: index, timestamp, data, hash and previous hash.
